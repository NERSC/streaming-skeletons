# Iperf3 Python Wrapper Configuration
# Override default settings with environment variables

# Network settings
PIPERF3_SERVER_HOST=node001.cluster.local
PIPERF3_PORT=5201

# Test parameters  
PIPERF3_DURATION=30
PIPERF3_BITRATE=10G
PIPERF3_PARALLEL=4
PIPERF3_PROTOCOL=tcp
PIPERF3_FORMAT=g

# Output settings
PIPERF3_OUTPUT_DIR=./results
PIPERF3_RUN_ID=auto

# Advanced settings
PIPERF3_VERBOSE=true
PIPERF3_JSON=true

# SLURM/HPC specific (these are typically set by the job scheduler)
# SLURM_JOB_ID=
# SLURM_JOB_NAME=
# SLURM_NODELIST=
# SLURM_NTASKS=
