# Global benchmark settings
PYZMQ_BENCH_NUM_PAIRS=1
PYZMQ_BENCH_SHORT_TEST=false

# Logging configuration
PYZMQ_BENCH_LOGGING__LEVEL=INFO
PYZMQ_BENCH_LOGGING__FORMAT="%(asctime)s [%(levelname)s] [%(name)s] %(message)s"

# Network settings
PYZMQ_BENCH_NETWORK__COORDINATOR_IP=127.0.0.1
PYZMQ_BENCH_NETWORK__COORDINATOR_ROUTER_PORT=5555
PYZMQ_BENCH_NETWORK__COORDINATOR_PUB_PORT=5556
PYZMQ_BENCH_NETWORK__DATA_PORT_START=6000

# Test matrix settings (JSON arrays for lists)
PYZMQ_BENCH_TEST_MATRIX__MESSAGE_COUNTS='[100001]'
PYZMQ_BENCH_TEST_MATRIX__MESSAGE_SIZES='[64, 256, 1024, 4096, 16384, 65536, 262144, 1048576, 4194304, 10485760]'
PYZMQ_BENCH_TEST_MATRIX__MAX_MESSAGE_SIZE=10485760
PYZMQ_BENCH_TEST_MATRIX__ZERO_COPY_OPTIONS='[true, false]'
PYZMQ_BENCH_TEST_MATRIX__PUB_SUB_OPTIONS='[false]'
PYZMQ_BENCH_TEST_MATRIX__SEND_HWM_VALUES='[100]'
PYZMQ_BENCH_TEST_MATRIX__RECV_HWM_VALUES='[100]'

# Worker settings
PYZMQ_BENCH_WORKER__SENDER_BIND=false
PYZMQ_BENCH_WORKER__SETUP_DELAY_S=1.0

# Output settings
PYZMQ_BENCH_OUTPUT__RESULTS_FILE=test_results.csv
PYZMQ_BENCH_OUTPUT__PLOT_OUTPUT_FILE=throughput_plot.png
PYZMQ_BENCH_OUTPUT__PLOT_SHOW=false
PYZMQ_BENCH_OUTPUT__PLOT_FIGURE_SIZE='[10, 6]'
PYZMQ_BENCH_OUTPUT__SAVE_INTERMEDIATE_RESULTS=true
